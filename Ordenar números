#include <iostream>
#include <stdlib.h>

using namespace std;

int main() {
  int num1, num2, num3;

  cout << "Escribe tres nÃºmeros --> ";
  cin >> num1 >> num2 >> num3;

  int temp;
  if (num1 > num2) {
    temp = num1;
    num1 = num2;
    num2 = temp;
  }
  if (num2 > num3) {
    temp = num2;
    num2 = num3;
    num3 = temp;
  }
  if (num1 > num2) {
    temp = num1;
    num1 = num2;
    num2 = temp;
  }

  cout << "Los nÃºmeros ordenados son " << num1 << ", " << num2 << " y " << num3 << endl;

  return 0;
}
